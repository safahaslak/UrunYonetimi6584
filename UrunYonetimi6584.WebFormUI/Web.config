	  </authorization>
  </system.web>
		<!--location ı da biz oluşturduk-->
	<location path="admin"> 
		<system.web>
			<authorization>
				<deny users="?"/>
				<!--deny users="?" admin klasörüne giriş yapmayan kullanıcıları kabul etme-engelle-->
			</authorization>
		</system.web>
	</location>
